@article{FRANCA2022110860,
title = {A machine learning strategy for computing interface curvature in Front-Tracking methods},
journal = {Journal of Computational Physics},
volume = {450},
pages = {110860},
year = {2022},
issn = {0021-9991},
doi = {https://doi.org/10.1016/j.jcp.2021.110860},
url = {https://www.sciencedirect.com/science/article/pii/S0021999121007555},
author = {Hugo L. França and Cassio M. Oishi},
keywords = {Machine learning, Front-Tracking, Curvature, Free surface flows, Marker-and-cell},
abstract = {In this work we have described the application of a machine learning strategy to compute the interface curvature in the context of a Front-Tracking framework. Based on angular information of normal and tangential vectors between marker points, the interface curvature is predicted using a neural network. The Front-Tracking-Machine-Learning method is validated using a sine wave and then applied in combination with a Marker-And-Cell method for solving a complex free surface flow. Our results indicate that it is feasible to employ machine learning concepts as an alternative approach for computing curvatures in Front-Tracking schemes.}
}

